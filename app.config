<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <add key="settings.ConfigFile" value="config.xml" />
  </appSettings>
	<system.diagnostics>
		<switches>
			<add name="Show" value="1" />
		</switches>
		<trace autoflush="true" indentsize="2">
			<listeners>
				<add name="RsdnLog"
					type="System.Diagnostics.TextWriterTraceListener" initializeData="rsdnnntp.log"/>
			</listeners>
		</trace>
	</system.diagnostics>
</configuration>